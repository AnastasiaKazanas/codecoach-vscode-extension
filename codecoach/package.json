{
  "name": "codecoach",
  "displayName": "CodeCoach",
  "description": "Ai-powered programming explanations and hints inside VS code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codecoach.setApiKey",
    "onCommand:codecoach.explainSelection",
    "onCommand:codecoach.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
  "commands": [
    { "command": "codecoach.explainSelection", "title": "CodeCoach: Explain Selection" },
    { "command": "codecoach.setApiKey", "title": "CodeCoach: Set Gemini API Key" },
    { "command": "codecoach.listGeminiModels", "title": "CodeCoach: List Gemini Models" }
  ]
},
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "openai": "^6.18.0"
  }
}
